{
  "name": "thatsnotmetalarchive",
  "version": "0.0.1",
  "description": "Show notes and links for every That's Not Metal podcast episode",
  "main": "index.js",
  "scripts": {
    "build": "yarn build-assets && yarn build-hugo",
    "build-assets": "webpack --env=prod -p",
    "build-hugo": "hugo",
    "start": "concurrently \"yarn start-assets\" \"yarn start-hugo\"",
    "start-assets": "webpack --env=dev --watch",
    "start-hugo": "hugo server -D",
    "test": "yarn test:js & yarn test:css",
    "test:css": "stylelint --config .stylelintrc.json assets/**/*.scss",
    "test:js": "eslint --config .eslintrc.json assets/**/*.js *.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomodwyer/thatsnotmetalarchive.git"
  },
  "author": "Tom O'Dwyer <git@tomodwyer.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomodwyer/thatsnotmetalarchive/issues"
  },
  "homepage": "https://github.com/tomodwyer/thatsnotmetalarchive#readme",
  "dependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.4.2",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jquery": "^3.3.1",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.19",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1",
    "sass-loader": "^6.0.6",
    "skel-framework": "git+https://github.com/ajlkn/skel.git",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^3.0.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
